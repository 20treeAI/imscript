CC="colorgcc -std=c99 -pedantic-errors -Wall -Wextra -Wshadow"
FLGS="-Wno-unused -g"
LIBS="iio.o -ltiff -lpng"
OUTDIR="bin/"

PROGS="synflow viewflow faxpb downsa component qeasy vecstack backflow rgbcube iminfo fnorm"

for i in $PROGS; do
	$CC $FLGS $i.c $LIBS -o ${OUTDIR}$i
done

